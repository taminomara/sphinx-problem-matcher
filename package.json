{
  "name": "sphinx-problem-matcher",
  "version": "1.0.0",
  "private": true,
  "description": "Attaches a problem matcher that looks for errors during Sphinx builds",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "tsc --noEmit",
    "package": "ncc build src/main.ts && cp src/problem-matcher.json dist/",
    "release": "npm run package && git add -f dist/",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taminomara/sphinx-problem-matcher.git"
  },
  "keywords": [
    "actions",
    "github",
    "problem-matcher",
    "annotations"
  ],
  "author": "taminomara",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^20.1.4",
    "@vercel/ncc": "^0.36.1",
    "github-actions-problem-matcher-typings": "^1.1.0",
    "jest": "^29.5.0",
    "jest-circus": "^29.5.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
}
